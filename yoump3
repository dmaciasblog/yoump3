clear
echo "##############################"
echo "#                            #"
echo "###     DMACIASBLOG.COM    ###"
echo "##############################"
echo "Descargando video espere"
youtube-dl $1
touch link
echo "$1" > link
num=$(cut -d= -f2 link)
echo -e -n "Dele nombre a su archivo de audio sin espacios: "
read name
ffmpeg -i *$num* "$name.mp3"
rm link *$num*
echo "Terminado"
